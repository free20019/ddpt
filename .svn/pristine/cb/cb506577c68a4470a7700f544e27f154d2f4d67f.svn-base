</div>
<div id="khb2Tabel"></div>
<div data-dojo-type="dijit/Dialog" data-dojo-id="khb2Dialog" id="khb2Dialog">
    <div data-dojo-type="dojox/layout/ContentPane" data-dojo-id="khb2DialogPanel" id="khb2DialogPanel"></div>
</div>
<script type="text/javascript">
    require(['app/html/czchtgl/khb2', "dojo/domReady!"], function (KHB2) {
        new KHB2();
    });
