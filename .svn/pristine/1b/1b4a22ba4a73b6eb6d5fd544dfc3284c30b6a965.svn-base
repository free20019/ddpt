<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>报表统计</title>
	<style>
		#gsbbtjTabel {
			height: calc(100% - 68px);
		}
		#gsbbtjTabels {
            height: 100%;
        }
		#gsbbtjDialog .dijitDialogPaneContent {
			padding: 0;
		}
		#gsbbtjDialogPanel {
			width: 600px;
			height: 300px;
		}
	</style>
</head>
<body>
<div class="" id="gsbbtjPanel" style="margin-bottom: 10px;">
	<div class="iFInlineItem">
		<label class="iFLabel">开始时间</label>
		<div class="iFInput" style="width: 160px;">
			<input type="text" id="gsbbtj-startTime">
		</div>
	</div>
	<div class="iFInlineItem">
		<label class="iFLabel">结束时间</label>
		<div class="iFInput" style="width: 160px;">
			<input type="text" id="gsbbtj-endTime">
		</div>
	</div>
	<div class="iFInlineItem" style="line-height: 30px;">
		<a href="javascript:void(0);" class="iFBtn iFColorOrange" id="gsbbtjQuery">查询</a>
		<a href="javascript:void(0);" class="iFBtn iFColorOrange" id="gsbbtj-dcsj">导出数据</a>
	</div>
</div>
<div class="">
	<div class="iFInlineItem">
		<label class="iFLabel">报警车辆</label>
		<div class="iFText gsbbtj-bjcl"></div>,
		<label class="iFLabel">总报警数</label>
		<div class="iFText gsbbtj-zbjs"></div>,
		<label class="iFLabel">真实报警数</label>
		<div class="iFText gsbbtj-zsbj"></div>,
		<label class="iFLabel">误报警数</label>
		<div class="iFText gsbbtj-wbj"></div>
	</div>
</div>
<div id="gsbbtjTabel"></div>
<div data-dojo-type="dijit/Dialog" data-dojo-id="gsbbtjDialog" id="gsbbtjDialog">
	<div data-dojo-type="dojox/layout/ContentPane" data-dojo-id="gsbbtjDialogPanel" id="gsbbtjDialogPanel"></div>
</div>
<script type="text/javascript">
$("#gsbbtj-startTime").val(setformat1(new Date(new Date()-1000*60*60*24)));
$("#gsbbtj-endTime").val(setformat1(new Date));
	require(['app/html/oa/bjtj/gsbbtj', "dojo/domReady!"], function (GSBBTJ) {
		new GSBBTJ();
	});
</script>
</body>
</html>