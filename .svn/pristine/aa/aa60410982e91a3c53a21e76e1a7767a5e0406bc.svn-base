<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>来电号码-选择客户</title>
    <style>
        #ldhmContainer {
            padding: 0;
            border: 1px solid #eeeeee;
            width: 100%;
            height: 100%;
        }
        #ldhm-centerPanel {
            padding: 0;
            width: 100%;
            height: 90%;
        }
        #ldhm-bottomPanel{
            text-align: center;
            height: 10%;
            padding-top: 5%;
        }
        #ldhmTable {
            width: calc(100% - 2px);
            height: calc(100% - 2px);
        }
    </style>
</head>
<body>

<div data-dojo-type="dijit/layout/BorderContainer" data-dojo-props="gutters:true, liveSplitters:false" id="ldhmContainer">
    <div data-dojo-type="dojox/layout/ContentPane" data-dojo-props="region:'center', splitter:false" id="ldhm-centerPanel">
        <div id="ldhmTable"></div>
    </div>
    <div data-dojo-type="dojox/layout/ContentPane" data-dojo-props="region:'bottom', splitter:false" id="ldhm-bottomPanel">
        <a href="javascript:void(0);" class="iFBtn iFColorOrange cancel" style="margin-right: 10px">新用户</a>
        <a href="javascript:void(0);" class="iFBtn iFColorOrange cancel" style="margin-left: 10px">选择用户</a>
    </div>
</div>
<script>
    require(['app/html/xhcdd/compile/ldhm_xz', "dojo/domReady!"],function(LDHM_XZ){
        new LDHM_XZ();
    });
</script>
</body>
</html>
