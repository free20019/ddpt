<script type="text/javascript">
var tcindex=1;
function fhcd(){
	if ((!!window.ActiveXObject || "ActiveXObject" in window)&&CTIProxy){
		CTIProxy.Logout();
	 }
	if(typeof(client)!="undefined"&&client&&$("#ddxpt-txzt").html()=="已连接"){
		tcindex=1;
		client.disconnect();
	}
	window.location.href="menu.html";
}
function fhdl(){
		layer.confirm('确定退出到登录页面？', {
			  btn: ['确定','取消'] //按钮
			}, function(){
				if ((!!window.ActiveXObject || "ActiveXObject" in window)&&CTIProxy){
					CTIProxy.Logout();
				 }
				if(typeof(client)!="undefined"&&client&&$("#ddxpt-txzt").html()=="已连接"){
					tcindex=1;
					client.disconnect();
				}
				window.location.href="login.html";
			}, function(){
		});
}
</script>
