<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>定位表</title>
    <style>
        #sjbTabel {
            height: calc(100% - 60px);
        }
        #sjbTabels {
            height: 100%;
        }
        #sjbDialog .dijitDialogPaneContent {
            padding: 0;
        }
        #sjbDialogPanel {
            width: 400px;
            height: 300px;
        }
    </style>
</head>
<body>
<div class="">
	<div class="iFInlineItem">
		<label class="iFLabel">关键字</label>
		<div class="iFInput" style="width: 160px;">
			<input type="text" id="findsjb_dd">
		</div>
	</div>
    <div class="iFInlineItem" style="line-height: 30px;">
    	<a href="javascript:void(0);" class="iFBtn iFColorOrange" id="sjbFind">查询</a>
        <a href="javascript:void(0);" class="iFBtn iFColorOrange" id="sjbAdd">添加</a>
        <a href="javascript:void(0);" class="iFBtn iFColorOrange" id="sjbEdit">修改</a>
        <a href="javascript:void(0);" class="iFBtn iFColorOrange" id="sjbDel">删除</a>
        <a href="javascript:void(0);" class="iFBtn iFColorOrange" id="sjbDc">导出</a>
    </div>
</div>
<div id="sjbTabel"></div>
<div data-dojo-type="dijit/Dialog" data-dojo-id="sjbDialog" id="sjbDialog">
    <div data-dojo-type="dojox/layout/ContentPane" data-dojo-id="sjbDialogPanel" id="sjbDialogPanel"></div>
</div>
<script type="text/javascript">
    require(['app/html/czchtgl/sjb', "dojo/domReady!"], function (SJB) {
        new SJB();
    });
</script>
</body>
</html>